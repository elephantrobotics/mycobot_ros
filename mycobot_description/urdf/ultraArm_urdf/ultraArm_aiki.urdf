    <parent link="base"/>
    <child link="link1"/>
    <origin xyz= "0 0 0.12" rpy = "3.1415926 0 0"/>  
  </joint>


  <joint name="joint2_to_joint1" type="revolute">
    <axis xyz="0 0 1"/>
