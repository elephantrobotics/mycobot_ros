    <child link="link2"/>
    <origin xyz= "0.03 0 0.01" rpy = " -1.5707963 3.1415926 3.1415926 "/>  
  </joint>


  <joint name="joint3_to_joint2" type="revolute">
    <axis xyz=" 0 0 1"/>
    <limit effort = "1000.0" lower = "0" upper = "1.0472" velocity = "0"/>
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz= "0.00 -0.12 0   " rpy = "0 0 0 "/>  
