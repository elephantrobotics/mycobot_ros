    <parent link="link5"/>
    <child link="link6"/>
    <origin xyz= "0 0.06 0" rpy = "-1.57080 0 0 "/>
  </joint>


  <joint name="gitpper_to_joint6output" type="fixed">
    <axis xyz="0 0 0"/>
