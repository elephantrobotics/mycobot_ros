    </collision>
  </link>

  <link name="link6">
    <visual>
      <geometry>
        <cylinder length="0.02" radius="0.026"/>
      </geometry>
      <material name = "grey">
        <color rgba = "0.5 0.5 0.5 1"/>
      </material>
    <origin xyz = "0 0 -0.01 " rpy = " 0 0 0"/>
    </visual>
       <collision>
      <origin xyz = "0.0 0 -0.01 " rpy = " 0 0 0"/>
      <geometry>
        <cylinder length="0.0225" radius="0.026"/>
