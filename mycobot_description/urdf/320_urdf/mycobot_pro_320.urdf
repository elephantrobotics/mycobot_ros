    </collision>
  </link>


  <link name="link5">
    <visual>
      <geometry>
       <mesh filename="package://mycobot_description/urdf/320_urdf/link5.dae"/>
      </geometry>
      <origin xyz = "0 0.002  0 " rpy = " 0  0 3.1415926"/>
