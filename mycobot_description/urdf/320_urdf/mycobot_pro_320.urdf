      </geometry>
      <origin xyz = "0 0 0 " rpy = " 0 0 0"/>
    </visual>
  </link>


  <joint name="joint2_to_joint1" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort = "1000.0" lower = "-3.14" upper = "3.14159" velocity = "0"/>
    <parent link="base"/>
    <child link="link1"/>
    <origin xyz= "0 0 0.17846" rpy = "0 0 0"/>
  </joint>


  <joint name="joint3_to_joint2" type="revolute">
    <axis xyz="0 0 1"/>
