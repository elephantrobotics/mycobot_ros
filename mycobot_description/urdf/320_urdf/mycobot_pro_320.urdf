    </collision>
  </link>



  <link name="link4">
    <visual>
      <geometry>
       <mesh filename="package://mycobot_description/urdf/320_urdf/link4.dae"/>
      </geometry>
      <origin xyz = "0 -0.001 0 " rpy = " 0 0 0"/>
    </visual>
    <collision>
